# Variables usadas en docker-compose.yml
# En Coolify, estas se definen como Magic Vars o Environment Variables

# --- Secrets (usar Magic Vars de Coolify) ---
SESSION_SECRET=
MAGIC_LINK_SECRET=
ENCRYPTION_KEY=
MANAGED_WORKER_SECRET=

# --- Postgres ---
POSTGRES_DB=trigger

# --- Registry externo (Tn-Registry) ---
DEPLOY_REGISTRY_HOST=
DEPLOY_REGISTRY_NAMESPACE=trigger
DEPLOY_REGISTRY_USERNAME=
DEPLOY_REGISTRY_PASSWORD=

# --- Otros opcionales ---
APP_LOG_LEVEL=info
TRIGGER_TELEMETRY_DISABLED=1
